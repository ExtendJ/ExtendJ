  /** Generate bytecode to load this field. */
  protected static void ASTNode.emitLoadField(CodeGeneration gen, Variable field,
      TypeDecl hostType) {
    if (field.hostType().isArrayDecl() && field.name().equals("length")) {
      gen.emit(Bytecode.ARRAYLENGTH);
      return;
    }
    String classname = hostType.constantPoolName();
    String desc = field.type().typeDescriptor();
    String name = field.name();
    int index = gen.constantPool().addFieldref(classname, name, desc);
    if (field.isStatic()) {
      gen.emit(Bytecode.GETSTATIC, field.type().variableSize()).add2(index);
    } else {
      gen.emit(Bytecode.GETFIELD, field.type().variableSize() - 1).add2(index);
    }
  }

