  /** Generate bytecode to store a field in the given hostType. */
  protected void ASTNode.emitStoreField(CodeGeneration gen, Variable var, TypeDecl hostType) {
    String classname = hostType.constantPoolName();
    String desc = var.type().typeDescriptor();
    String name = var.name();
    int index = gen.constantPool().addFieldref(classname, name, desc);
    if (var.isStatic()) {
      gen.emit(Bytecode.PUTSTATIC, -var.type().variableSize()).add2(index);
    } else {
      gen.emit(Bytecode.PUTFIELD, -var.type().variableSize() - 1).add2(index);
    }
  }

